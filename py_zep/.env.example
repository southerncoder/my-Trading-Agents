# Environment variables for Zep Graphiti service
# Copy this file to .env and customize with your values

# LM Studio configuration (for local LLM)
OPENAI_API_KEY=your-api-key-here
OPENAI_BASE_URL=http://your-lm-studio-host:port/v1
OPENAI_MODEL=your-model-name

# Neo4j configuration
NEO4J_URI=bolt://your-neo4j-host:port
NEO4J_USER=your-username
NEO4J_PASSWORD=your-secure-password

# Service configuration
ZEP_SERVICE_HOST=your-host
ZEP_SERVICE_PORT=your-port
ZEP_SERVICE_DEBUG=false

# Logging
LOG_LEVEL=INFO

# Telemetry
GRAPHITI_TELEMETRY_ENABLED=false