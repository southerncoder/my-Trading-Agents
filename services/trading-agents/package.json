{
  "name": "my-trading-agents-ts",
  "version": "0.1.0",
  "type": "module",
  "description": "Multi-Agents LLM Financial Trading Framework - TypeScript Implementation",
  "main": "dist/index.js",
  "scripts": {
    "build": "vite build",
    "dev": "vite-node src/index.ts",
    "start": "node dist/cli/main.js",
    "watch": "vite build --watch",
    "cli": "node cli.js",
    "cli:menu": "node cli.js menu",
    "cli:analyze": "node cli.js analyze",
    "cli:export": "node cli.js export",
    "cli:historical": "node cli.js historical",
    "cli:config": "node cli.js config",
    "cli:dev": "vite-node src/cli/main.ts",
    "cli:dev:menu": "vite-node src/cli/main.ts menu",
    "cli:build": "vite build && node dist/index.js",
    "cli:test": "vite-node tests/cli/automated-cli-test.ts",
    "cli:test:watch": "nodemon --watch src --watch tests/cli --ext ts --exec \"npx vite-node tests/cli/automated-cli-test.ts\"",
    "test": "npm run test:workflow",
    "test:workflow": "vite-node scripts/run-workflow-tests.ts",
    "test:performance": "jest tests/performance/comprehensive-performance.test.ts tests/performance/performance-regression.test.ts --testTimeout=30000",
    "test:performance:ci-cd": "jest tests/performance/ci-cd-performance.test.ts --testTimeout=30000",
    "test:performance:regression": "jest tests/performance/performance-regression.test.ts --testTimeout=30000",
    "test:performance:comprehensive": "jest tests/performance/comprehensive-performance.test.ts --testTimeout=30000",
    "test:all": "npm run test:workflow && npm run test:performance",
    "services:start": "powershell -ExecutionPolicy Bypass -File ../zep_graphiti/start-zep-services.ps1",
    "services:fresh": "powershell -ExecutionPolicy Bypass -File ../zep_graphiti/start-zep-services.ps1 -Fresh",
    "services:start-simple": "../zep_graphiti/start-services-simple.bat",
    "services:stop": "powershell -Command \"docker-compose -f ../zep_graphiti/docker-compose.yml down\"",
    "services:status": "powershell -Command \"docker-compose -f ../zep_graphiti/docker-compose.yml ps\"",
    "test:full": "npm run services:start && npm run test-enhanced && npm run test-cli-components && npm run test-memory",
    "dev:full": "npm run services:start && npm run dev",
    "cli:full": "npm run services:start && npm run cli",
    "check-langgraph": "vite-node debugScripts/check-langgraph.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "type-check": "tsc --noEmit",
    "quality": "npm run type-check && npm run lint && npm run format:check",
    "quality:fix": "npm run format && npm run lint:fix",
    "audit:security": "npm audit --audit-level moderate",
    "audit:deps": "npm outdated",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "smoke": "npm run type-check && npm test || true",
    "test:manual:brave-news": "vite-node tests/manual/brave-news-provider.manual.test.js",
    "test:manual:learning": "vite-node tests/manual/learning-market-analyst.manual.test.js",
    "test:manual:lmstudio-connection": "vite-node tests/manual/lmstudio-connection.manual.test.ts",
    "test:manual:lmstudio-urls": "vite-node tests/manual/lmstudio-urls.manual.test.js",
    "test:manual:docker-secrets": "node ../../docker/tests/docker-secrets.manual.test.js",
    "test:manual:redis": "vite-node scripts/test-redis-connection.ts",
    "test:manual:all": "npm run test:manual:brave-news && npm run test:manual:learning && npm run test:manual:lmstudio-connection && npm run test:manual:lmstudio-urls && npm run test:manual:redis && npm run test:manual:docker-secrets",
    "deps:check": "npm outdated && npm audit",
    "deps:update": "powershell -ExecutionPolicy Bypass -File ../../scripts/dependency-update.ps1",
    "deps:update:security": "powershell -ExecutionPolicy Bypass -File ../../scripts/dependency-update.ps1 -Security",
    "deps:update:major": "powershell -ExecutionPolicy Bypass -File ../../scripts/dependency-update.ps1 -Major",
    "deps:monitor": "powershell -ExecutionPolicy Bypass -File ../../scripts/dependency-monitor.ps1",
    "deps:rollback": "powershell -ExecutionPolicy Bypass -File ../../scripts/dependency-rollback.ps1",
    "deps:breaking-changes": "powershell -ExecutionPolicy Bypass -File ../../scripts/dependency-breaking-change-detector.ps1 -Service trading-agents -AllPackages",
    "deps:api-compatibility": "powershell -ExecutionPolicy Bypass -File ../../scripts/dependency-api-compatibility-validator.ps1 -Service trading-agents -AllPackages",
    "deps:maintenance": "powershell -ExecutionPolicy Bypass -File ../../scripts/dependency-maintenance-scheduler.ps1",
    "deps:maintenance:status": "powershell -ExecutionPolicy Bypass -File ../../scripts/dependency-maintenance-scheduler.ps1 -Action status",
    "deps:maintenance:report": "powershell -ExecutionPolicy Bypass -File ../../scripts/dependency-maintenance-scheduler.ps1 -Action report",
    "deps:setup": "powershell -ExecutionPolicy Bypass -File ../../scripts/dependency-automation-setup.ps1",
    "deps:setup:all": "powershell -ExecutionPolicy Bypass -File ../../scripts/dependency-automation-setup.ps1 -All",
    "deps:test": "npm run test -- tests/dependency-automation/dependency-automation.test.ts"
  },
  "bin": {
    "trading-agents": "./cli.js"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.17",
    "@getzep/zep-js": "^2.0.2",
    "@langchain/anthropic": "^0.3.30",
    "@langchain/google-genai": "^0.2.18",
    "@langchain/langgraph": "^0.4.9",
    "@langchain/openai": "^0.6.15",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.65.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.206.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.206.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.206.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-logs": "^0.206.0",
    "@opentelemetry/sdk-metrics": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.206.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@types/nodemailer": "^7.0.2",
    "@types/opossum": "^8.1.9",
    "ai": "^5.0.68",
    "alphavantage": "^2.5.0",
    "axios": "^1.12.2",
    "boxen": "^8.0.1",
    "cacheable": "^2.1.0",
    "chalk": "^5.6.2",
    "cheerio": "^1.1.2",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "exponential-backoff": "^3.1.3",
    "graphzep": "^0.1.0",
    "inquirer": "^12.9.4",
    "langchain": "^0.3.35",
    "lodash": "^4.17.21",
    "lru-cache": "^11.2.1",
    "ml-cross-validation": "^1.3.0",
    "ml-matrix": "^6.12.1",
    "ml-regression": "^6.3.0",
    "newsapi": "^2.4.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.9",
    "ollama-ai-provider-v2": "^1.3.1",
    "opossum": "^9.0.0",
    "ora": "^8.2.0",
    "p-retry": "^7.0.0",
    "pg": "^8.16.3",
    "pgvector": "^0.2.1",
    "prom-client": "^15.1.3",
    "redis": "^5.8.3",
    "rimraf": "^6.0.1",
    "twilio": "^5.10.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vite-node": "^3.2.4",
    "vite-tsconfig-paths": "^5.1.4",
    "winston": "^3.18.3",
    "yahoo-finance2": "^2.13.3",
    "zod": "^4.1.8"
  },
  "overrides": {
    "form-data": "^4.0.4"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.7.2",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^9.37.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6"
  },
  "keywords": [
    "trading",
    "agents",
    "llm",
    "ai",
    "financial",
    "typescript",
    "langgraph",
    "langchain"
  ],
  "author": "southernCoder",
