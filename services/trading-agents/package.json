{
  "name": "my-trading-agents-ts",
  "version": "0.1.0",
  "type": "module",
  "description": "Multi-Agents LLM Financial Trading Framework - TypeScript Implementation",
  "main": "dist/index.js",
  "scripts": {
    "build": "vite build",
    "dev": "vite-node src/index.ts",
    "start": "node dist/cli/main.js",
    "watch": "vite build --watch",
    "cli": "node cli.js",
    "cli:menu": "node cli.js menu",
    "cli:analyze": "node cli.js analyze",
    "cli:export": "node cli.js export",
    "cli:historical": "node cli.js historical",
    "cli:config": "node cli.js config",
    "cli:dev": "vite-node src/cli/main.ts",
    "cli:dev:menu": "vite-node src/cli/main.ts menu",
    "cli:build": "vite build && node dist/index.js",
  "test": "npm run test:workflow",
  "test:workflow": "vite-node scripts/run-workflow-tests.ts",
  "test:all": "npm run test:workflow",
  "services:start": "powershell -ExecutionPolicy Bypass -File ../zep_graphiti/start-zep-services.ps1",
  "services:fresh": "powershell -ExecutionPolicy Bypass -File ../zep_graphiti/start-zep-services.ps1 -Fresh",
  "services:start-simple": "../zep_graphiti/start-services-simple.bat",
  "services:stop": "powershell -Command \"docker-compose -f ../zep_graphiti/docker-compose.yml down\"",
  "services:status": "powershell -Command \"docker-compose -f ../zep_graphiti/docker-compose.yml ps\"",
    "test:full": "npm run services:start && npm run test-enhanced && npm run test-cli-components && npm run test-memory",
    "dev:full": "npm run services:start && npm run dev",
    "cli:full": "npm run services:start && npm run cli",
    "check-langgraph": "vite-node debugScripts/check-langgraph.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "type-check": "tsc --noEmit",
    "quality": "npm run type-check && npm run lint && npm run format:check",
    "quality:fix": "npm run format && npm run lint:fix",
    "audit:security": "npm audit --audit-level moderate",
    "audit:deps": "npm outdated",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "smoke": "npm run type-check && npm test || true"
    ,"test:manual:brave-news": "vite-node tests/manual/brave-news-provider.manual.test.js"
    ,"test:manual:learning": "vite-node tests/manual/learning-market-analyst.manual.test.js"
    ,"test:manual:lmstudio-connection": "vite-node tests/manual/lmstudio-connection.manual.test.ts"
    ,"test:manual:lmstudio-urls": "vite-node tests/manual/lmstudio-urls.manual.test.js"
    ,"test:manual:docker-secrets": "node ../../docker/tests/docker-secrets.manual.test.js"
    ,"test:manual:all": "npm run test:manual:brave-news && npm run test:manual:learning && npm run test:manual:lmstudio-connection && npm run test:manual:lmstudio-urls && npm run test:manual:docker-secrets"
  },
  "bin": {
    "trading-agents": "./cli.js"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.17",
