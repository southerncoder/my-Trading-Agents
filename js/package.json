{
  "name": "trading-agents-ts",
  "version": "0.1.0",
  "type": "module",
  "description": "Multi-Agents LLM Financial Trading Framework - TypeScript Implementation",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node --project tsconfig.dev.json src/index.ts",
    "start": "node dist/index.js",
    "watch": "tsc --watch",
    "cli": "ts-node src/cli/main.ts analyze",
    "cli:analyze": "ts-node src/cli/main.ts analyze",
    "cli:build": "tsc && node dist/cli/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test-graph": "ts-node --project tsconfig.dev.json src/test-graph.ts",
    "test-langgraph": "node tests/test-langgraph.js",
    "test-enhanced": "node tests/test-enhanced-graph.js",
    "test-cli": "node tests/test-cli-integration.js",
    "test-cli-components": "node tests/test-cli-components.js",
    "test-api-integrations": "node tests/test-api-integrations.js",
    "test-focused-apis": "node tests/test-focused-apis.js",
    "test-simple-apis": "node tests/test-simple-apis.js",
    "test-minimal-apis": "node tests/test-minimal-apis.js",
    "check-langgraph": "node debugScripts/check-langgraph.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "type-check": "tsc --noEmit",
    "quality": "npm run type-check && npm run lint && npm run format:check",
    "quality:fix": "npm run format && npm run lint:fix",
    "audit:security": "npm audit --audit-level moderate",
    "audit:deps": "npm outdated",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build"
  },
  "bin": {
    "trading-agents": "dist/cli/main.js"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.2.0",
    "@langchain/google-genai": "^0.0.20",
    "@langchain/langgraph": "^0.0.30",
    "@langchain/openai": "^0.2.0",
    "axios": "^1.7.2",
    "boxen": "^7.1.1",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "inquirer": "^9.3.7",
    "langchain": "^0.2.0",
    "lodash": "^4.17.21",
    "lru-cache": "^11.1.0",
    "node-fetch": "^3.3.2",
    "ora": "^8.2.0",
    "redis": "^4.7.0",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/inquirer": "^8.2.12",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.6",
    "@types/lru-cache": "^7.10.9",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.5.3"
  },
  "keywords": [
    "trading",
    "agents",
    "llm",
    "ai",
    "financial",
    "typescript",
    "langgraph",
    "langchain"
  ],
  "author": "TauricResearch",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/southerncoder/my-Trading-Agents.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
